<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net.hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.rhcloud.javawojtekel.pinezki.UzytkownikGrupaModel" table="UzytkownicyGrupy">
        <id name="identyfikator" type="int">
            <column name="identyfikator" />
            <generator class="identity" />
        </id>
        <many-to-one name="uzytkownik" column="uzytkownik" class="com.rhcloud.javawojtekel.pinezki.ModelUzytkownik" />
        <many-to-one name="grupa" column="grupa" class="com.rhcloud.javawojtekel.pinezki.GrupaModel" />
    </class>
</hibernate-mapping>
